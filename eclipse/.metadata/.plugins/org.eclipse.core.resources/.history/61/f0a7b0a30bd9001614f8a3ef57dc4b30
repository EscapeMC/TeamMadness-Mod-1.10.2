package com.github.escapemc.teammadnessmod.items;

import com.github.escapemc.teammadnessmod.Reference;

import net.minecraft.item.ItemSpade;

public class madness_shovel extends ItemSpade {

	public madness_shovel(ToolMaterial material) {
		super(material);
		
		setUnlocalizedName(Reference.ModItems.MADNESS_SHOVEL.getUnlocalizedName());
		setRegistryName(Reference.ModItems.MADNESS_SHOVEL.getRegistryName());

		
	}

}
