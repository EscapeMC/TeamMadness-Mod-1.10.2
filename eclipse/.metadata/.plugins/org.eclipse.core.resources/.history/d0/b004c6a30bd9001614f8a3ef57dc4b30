package com.github.escapemc.teammadnessmod.items;

import com.github.escapemc.teammadnessmod.Reference;

import net.minecraft.item.ItemFood;

public class chicken_taco extends ItemFood {

	public chicken_taco(int amount, float saturation, boolean isWolfFood) {
		super(amount, saturation, isWolfFood);
		
		setUnlocalizedName(Reference.ModItems.CHICKEN_TACO.getUnlocalizedName());
		setRegistryName(Reference.ModItems.CHICKEN_TACO.getRegistryName());

		
	}

}
