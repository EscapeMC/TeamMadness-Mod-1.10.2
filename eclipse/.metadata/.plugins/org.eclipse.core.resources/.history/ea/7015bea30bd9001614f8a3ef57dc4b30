package com.github.escapemc.teammadnessmod.items;

import com.github.escapemc.teammadnessmod.Reference;

import net.minecraft.item.ItemSword;

public class escapemc_sword extends ItemSword {

	public escapemc_sword(ToolMaterial material) {
		super(material);
		
		setUnlocalizedName(Reference.ModItems.ESCAPEMC_SWORD.getUnlocalizedName());
		setRegistryName(Reference.ModItems.ESCAPEMC_SWORD.getRegistryName());

		
	}

}
