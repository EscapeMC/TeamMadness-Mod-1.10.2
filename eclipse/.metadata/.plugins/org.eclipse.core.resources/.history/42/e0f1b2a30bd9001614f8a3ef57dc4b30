package com.github.escapemc.teammadnessmod.items;

import com.github.escapemc.teammadnessmod.Reference;

import net.minecraft.item.ItemPickaxe;

public class mushrromstew_pickaxe extends ItemPickaxe {

	public mushrromstew_pickaxe(ToolMaterial material) {
		super(material);
		
		setUnlocalizedName(Reference.ModItems.MUSHRROMSTEW_PICKAXE.getUnlocalizedName());
		setRegistryName(Reference.ModItems.MUSHRROMSTEW_PICKAXE.getRegistryName());

	}

}
