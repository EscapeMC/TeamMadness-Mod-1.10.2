package com.github.escapemc.teammadnessmod.items;

import com.github.escapemc.teammadnessmod.Reference;

import net.minecraft.item.ItemSword;

public class diamondtips_sword extends ItemSword {

	public diamondtips_sword(ToolMaterial material) {
		super(material);
		
		setUnlocalizedName(Reference.ModItems.DIAMONDTIPS_SWORD.getUnlocalizedName());
		setRegistryName(Reference.ModItems.DIAMONDTIPS_SWORD.getRegistryName());

		
	}

}
