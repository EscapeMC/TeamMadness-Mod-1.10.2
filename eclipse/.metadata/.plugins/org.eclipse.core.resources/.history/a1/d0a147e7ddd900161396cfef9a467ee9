package com.github.escapemc.teammadnessmod.items;

import com.github.escapemc.teammadnessmod.Reference;

import net.minecraft.item.ItemAxe;

public class madness_axe extends ItemAxe {

	public madness_axe(ToolMaterial material, float damage, float speed) {
		super(material, damage, speed);
		
		setUnlocalizedName(Reference.ModItems.MADNESS_AXE.getUnlocalizedName());
		setRegistryName(Reference.ModItems.MADNESS_AXE.getRegistryName());

		
	}

}
