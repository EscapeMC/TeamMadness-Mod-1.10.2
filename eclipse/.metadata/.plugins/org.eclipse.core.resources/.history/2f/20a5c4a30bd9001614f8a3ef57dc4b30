package com.github.escapemc.teammadnessmod.items;

import com.github.escapemc.teammadnessmod.Reference;

import net.minecraft.item.ItemSword;

public class mushrromstew_sword extends ItemSword {

	public mushrromstew_sword(ToolMaterial material) {
		super(material);
		
		setUnlocalizedName(Reference.ModItems.MUSHRROMSTEW_SWORD.getUnlocalizedName());
		setRegistryName(Reference.ModItems.MUSHRROMSTEW_SWORD.getRegistryName());

	}

}
