package com.github.escapemc.teammadnessmod.items;

import com.github.escapemc.teammadnessmod.Reference;

import net.minecraft.item.ItemPickaxe;
import net.minecraft.item.Item.ToolMaterial;

public class ianplaysyt_pickaxe extends ItemPickaxe {

	public ianplaysyt_pickaxe(ToolMaterial material) {
		super(material);
		
		setUnlocalizedName(Reference.ModItems.IANPLAYSYT_PICKAXE.getUnlocalizedName());
		setRegistryName(Reference.ModItems.IANPLAYSYT_PICKAXE.getRegistryName());

		
	}

}
