package com.github.escapemc.teammadnessmod.items;

import com.github.escapemc.teammadnessmod.Reference;

import net.minecraft.inventory.EntityEquipmentSlot;
import net.minecraft.item.ItemArmor;

public class mushrromstew_armor extends ItemArmor {

	public mushrromstew_armor(ArmorMaterial materialIn, int renderIndexIn, EntityEquipmentSlot equipmentSlotIn) {
		super(materialIn, renderIndexIn, equipmentSlotIn);

	
		if(equipmentSlotIn == EntityEquipmentSlot.HEAD) {
			setUnlocalizedName(Reference.ModItems.MUSHRROMSTEW_HELMET.getUnlocalizedName());
			setRegistryName(Reference.ModItems.MUSHRROMSTEW_HELMET.getRegistryName());

		
		
		}
			
		if(equipmentSlotIn == EntityEquipmentSlot.CHEST) {
			setUnlocalizedName(Reference.ModItems.MUSHRROMSTEW_CHESTPLATE.getUnlocalizedName());
			setRegistryName(Reference.ModItems.MUSHRROMSTEW_CHESTPLATE.getRegistryName());

		
		
		}

		if(equipmentSlotIn == EntityEquipmentSlot.LEGS) {
			setUnlocalizedName(Reference.ModItems.MUSHRROMSTEW_LEGGINGS.getUnlocalizedName());
			setRegistryName(Reference.ModItems.MUSHRROMSTEW_LEGGINGS.getRegistryName());

		
		
		}

		if(equipmentSlotIn == EntityEquipmentSlot.FEET) {
			setUnlocalizedName(Reference.ModItems.MUSHRROMSTEW_BOOTS.getUnlocalizedName());
			setRegistryName(Reference.ModItems.MUSHRROMSTEW_BOOTS.getRegistryName());

		
		
		}

		
		
		
	}
	
}
