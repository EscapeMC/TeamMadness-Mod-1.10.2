package com.github.escapemc.teammadnessmod;

import com.github.escapemc.teammadnessmod.init.ModItems;

import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;

public class TeamMadnessModItemsTab extends CreativeTabs {

	public TeamMadnessModItemsTab(String label) {
		super(label);
	}

	@Override
	public Item getTabIconItem() {
		return ModItems.madness_sword;
	}
	
	
}
